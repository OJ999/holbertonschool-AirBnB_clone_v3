{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/structure": [
      "default",
      {
        "list": "off",
        "listitem": "off"
      }
    ],
    "meta-viewport": "off",
    "meta-charset-utf-8": "off",
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-ms-filter"
        ]
      }
    ],
    "no-protocol-relative-urls": "off",
    "axe/name-role-value": "off",
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-required-children": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ]
  }
}